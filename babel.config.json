{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": false,
                "targets": "supports rtcpeerconnection",
                "bugfixes": true,
                "exclude": [
                    "@babel/plugin-transform-regenerator"
                ]
            }
        ],
        "solid"
    ],
    "plugins": [
        [
            "module:fast-async",
            {
                "useRuntimeModule": true
            }
        ],
        "babel-plugin-solid-undestructure",
        "solid-styled-jsx/babel"
    ]
}
